#include<stdio.h>
main()
{
	int a[5],b[5],c[10];
	int i,n;
	printf("first array");
	for(i=0;i<=4;i++)
	{
		scanf("%d",&a[i]);
		c[i]=a[i];
	}
	n=i;
	printf("second array");
	for(i=0;i<=4;i++)
	{
		scanf("%d",&b[i]);
		c[n]=b[i];
		n++;
	}
	printf("merged array:");
	for(i=0;i<n;i++)
	{
		printf("%d\t",c[i]);
	}
}
